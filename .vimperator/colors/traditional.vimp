"
" #00aa90 Aomidori
" #08192d Kachi-iro
" #2e5c6e Omeshionando
" #2ea9df Tsuyukusa-iro
" #bdc0ba Shironezumi
" #cc543a Ake
" #e98b2a Beniukon
" #fcfaf2 Shironeri
" #fffffb Gofun
"

"
" Hint
"
highlight Hint <<CSS
  width: 2em;
  font-family: monospace;
  color: #00aa90;
  text-align: center;
  background-color: #fcfaf2;
  border: 2px solid #00aa90;
  border-radius: 4px;
CSS
highlight HintElem <<CSS
  /* no style */
CSS
highlight HintActive <<CSS
  color: #cc543a;
CSS

"
" Frame
"
highlight FrameIndicator -append <<CSS
  background-color: #08192d;
CSS

"
" Separator
"
highlight ContentSeparator <<CSS
  /* no style */
CSS

"
" Normal
"
highlight Normal <<CSS
  /* no style */
CSS

"
" Completions
"
highlight CompGroup <<CSS
  /* no style */
CSS
highlight CompGroup:not(:first-of-type) <<CSS
  /* no style */
CSS
highlight CompTitle <<CSS
  height: 2em;
  font-family: sans-serif;
  color: #fcfaf2;
  background-color: #08192d;
CSS
highlight CompTitle>* <<CSS
  text-align: center;
CSS
highlight CompItem>* <<CSS
  /* no style */
CSS
highlight CompItem:nth-child(even) <<CSS
  background-color: #fffffb;
CSS
highlight CompItem[selected] <<CSS
  background-color: #bdc0ba;
CSS
highlight CompDesc -append <<CSS
  padding-left: 1em;
  color: inherit;
CSS
highlight Normal>*[key=noCompletions] <<CSS
  height: 2em;
  font-family: sans-serif;
  color: #fcfaf2;
  text-align: center;
  background-color: #08192d;
CSS
highlight Title <<CSS
  /* no style */
CSS
highlight URL <<CSS
